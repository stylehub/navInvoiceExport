{
    "name": "dream/navinvoiceexport",
    "description": "Model and serializer for exporting invoice data for the Hungarian Tax and Customs Authority",
    "type": "library",
    "authors": [
        {
            "name": "Szigetvári Áron"
        }
    ],
    "require": {
        "goetas/xsd2php": "dev-master",
        "goetas/xsd-reader": "2.*@dev",
        "jms/serializer": "xsd2php-dev as 0.18.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/goetas/serializer.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/AronSzigetvari/xsd2php.git"
        }
    ],
    "scripts": {
        "build-classes": "xsd2php convert:php szamlaexport.xsd --ns-map \"http://schemas.nav.gov.hu/2013/szamla;Dream/NavInvoiceExport/Data/\" --ns-dest \"Dream/NavInvoiceExport/Data/;src/Data\" --naming-strategy \"short\"",
        "build-meta": "xsd2php convert:jms-yaml szamlaexport.xsd --ns-map \"http://schemas.nav.gov.hu/2013/szamla;Dream/NavInvoiceExport/Data/\" --ns-dest \"Dream/NavInvoiceExport/Data/;res/Metadata/JMS\" --naming-strategy \"short\""
    },
    "autoload": {
        "psr-4": {
            "Dream\\NavInvoiceExport\\": "src/"
        }
    }
}